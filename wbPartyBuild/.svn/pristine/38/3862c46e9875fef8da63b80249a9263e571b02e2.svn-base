#onlinetest{padding:8px 54px 0 54px;}
.onlinetest-tt{text-align: center;font-size:24px;line-height:60px;}
.onlinetest-subtt{text-align: center;font-size:12px;line-height:22px;color:#939393;}
.onlinetest-subtt strong{color:#ff9c00;font-style: normal;}
.onlinetest-controll{padding:10px;margin:12px 0;line-height:30px;font-size:14px;background:#f6f6f6;}
.onlinetest-controll strong{font-size:24px;font-weight: normal;color:#b40100;}
.onlinetest-controll .btngroup{float: right;text-align: right;line-height:28px;}
.onlinetest-controll .btngroup a{margin-left:6px;color:#b40100;}
.btn-white{display:inline-block;background-color: #fbfbfb;padding:0 22px;border: 1px solid #999;
	    filter:progid:DXImageTransform.Microsoft.Gradient(gradientType=0,startColorStr=#ffffff,endColorStr=#dddddd);
	    background: -webkit-linear-gradient(#ffffff, #dddddd);
	    background: -o-linear-gradient(#ffffff, #dddddd);
	    background: linear-gradient(#ffffff, #dddddd);}
.btn-white:hover {background: #EFEFEF;}
.btn-pause span+span{display: none;}
.btn-pause.pausing span+span{display: block;}
.btn-pause.pausing span:first-child{display: none;}
.onlinetest-controll .btngroup .btn-pause.disabled,.onlinetest-controll .btngroup .btn-their.disabled{background: #E9E9E9;color:#999;border-color:#AAA;}
.onlinetest-ct{border:1px solid #edd470;padding:12px 0;font-size:16px;}
.question{display: none;}
.question.current{display: block;}
.question-title{color: #6c4502;line-height:36px;padding:0 15px;}
.question-options,.anwsergroup{list-style-type:upper-latin;padding:0 12px 0 60px;margin:12px 0;}
.trueorfalse .question-options{list-style-type:none;}
.anwsergroup{list-style-type:lower-roman;}
.question-options li{position: relative;line-height:32px;padding-left: 6px;cursor:pointer;}
.anwsergroup li {position: relative;padding-left: 6px;}
.question-options li:hover{background-color:#f2f2f2;}
.question-options li:before,.anwsergroup li:before{content:"";display: inline-block;width:18px;height:32px;background:url('../../image/right-wrong.png') no-repeat 0px 8px;position: absolute;left:-48px;}
.question-options .selected:before{background-position:-64px 8px;}
.question-options .right:before{background-position:-21px 8px;}
.morechoice .selected.right:before{background-position:-85px 8px;}
.question-options .wrong:before{background-position:-21px 8px;}
.question-options .selected.wrong:before{background-position:-43px 8px;}
.anwsergroup .right:before{background-position:-21px 8px;}
.anwsergroup .wrong:before{background-position:-43px 8px;}
.anwsergroup li{margin-bottom:12px;}
.anwsergroup input{width:786px;max-width:100%;height:30px;box-sizing:content-box;border:1px solid transparent;border-bottom:1px solid #777;line-height:30px;font-size:16px;}
.question .btn-submit{line-height:26px;margin-left: 16px;}
.disabled .btn-submit{background: #E9E9E9;color:#999;border-color:#AAA;}
.singlechoice .btn-submit{display: none;}
.trueorfalse .btn-submit{display: none;}
.question-result{color:#b40100;line-height:38px;padding:0 14px;background-color:#f6f6f6;border:1px dashed #edd470;margin: 18px 12px;}
.question-result a{text-decoration: underline;color:#828282;font-size:14px;margin-left:10px;}
.onlinetest-ft{background-color:#f6f6f6;font-size:16px;line-height:30px;}
.onlinetest-guide{padding:15px 15px 0 15px;}
.onlinetest-ft .btn-white{margin-right: 6px;}
.onlinetest-navlist{margin:16px 8px 6px 15px;}
.onlinetest-navlist li{float: left;margin:0 6px 10px 0;}
.onlinetest-navlist li a{display: inline-block;width: 34px;height:34px;border:1px solid #9a9a9a;color:#555;text-align: center;line-height: 34px;background-color:#fff;border-radius: 2px;-webkit-transition:all 0.6s;
-o-transition:all 0.6s;
transition:all 0.6s;}
.onlinetest-navlist li a:hover{border-color:#2196F3;color:#2196F3;}
.onlinetest-navlist li.wrong a{background-color:#D21D1C;color:#fff;border:1px solid #D21D1C	;}
.onlinetest-navlist li.right a{background-color:#27A72C;color:#fff;border:1px solid #27A72C;}
.onlinetest-statistics{text-align: center;font-size: 14px;}
.onlinetest-statistics em{color: #13a100;font-style: normal;font-weight: bold;}
.onlinetest-statistics strong{color: #dc0100;font-weight: bold;}
.analysislist{display: none;}


.mylayer-main{vertical-align: middle;position: relative;border: 1px solid #d8a400;}
.mylayer-hd{content: "";clear: both;background: #fff;font-size: 20px;line-height:34px;}
.mylayer-main>.mylayer-close{color:#fff;background-color: #b06a2e;display: inline-block;width:24px;height:24px;border-radius: 4px;font-size:22px;line-height:22px;cursor:pointer;position: absolute;top:10px;right:10px;text-align: center;}
.mylayer-main .btns{padding:0 !important;border-top:1px solid #B06A2E;height:40px;margin-top: 15px;}
.mylayer-main .btns a{float: left;width:50%;line-height:40px;text-align:center;color:#B06A2E;font-size:18px;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;background-color:#FBF6F2;}
.mylayer-main .btns a:hover{background-color:#B06A2E;color:#FFF; }
.mylayer-main .btns a+a{border-left:1px solid #B06A2E;}

#mylayer1{text-align: left;padding:30px;background: #fff;}
#mylayer1 .tt,#mylayer1 .subtt{color:#b16a2a;}
#mylayer1 .tt{font-size:36px;font-weight:bold;letter-spacing:2px;line-height:1;}
#mylayer1 .subtt{font-size:8px;line-height:24px;}
#mylayer1 .ct{width:458px;min-height:100px;max-height:400px;border:1px solid #d8a400;overflow: auto;margin-top:16px;padding:14px 20px;font-size:14px;}

#mylayer1 .ct p{line-height:26px;text-indent: 2em;}
#mylayer1 .ct .opt{font-weight: bold;text-indent: 0;margin-top:4px;}

.mylayer-msg{padding-top:20px;width:358px;font-size:16px;line-height:30px;color:#222;overflow: hidden;overflow: visible;}
.mylayer-msg>div:first-child{font-size:18px;padding:0 30px;color:#9C5E28;}
.mylayer-msg>div:first-child+div,.mylayer-msg>div:first-child+div+div{text-indent:2em;padding:2px 30px;font-size:16px;line-height:24px;}
.mylayer-msg .wrong{color:#dc0100;font-size:20px;margin:0 4px;}
.mylayer-msg .right{color:#13a100;font-size:20px;margin:0 4px;}
.mylayer-msg .totalscore,.mylayer-msg .totaltime{color:#185F86;}
.mylayer-msg .usetime{color: #029289;}