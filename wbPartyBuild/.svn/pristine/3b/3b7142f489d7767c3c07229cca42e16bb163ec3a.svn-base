package com.wb.web.study.dto.topic;

import com.wb.core.common.dto.BaseQueryDTO;

public class TopicQueryDTO extends BaseQueryDTO{
	
	private Long categoryId;
	private String questionText;
	private Integer topicType;
	private String choosentopic;
		
	public Integer getTopicType() {
		return topicType;
	}
	public void setTopicType(Integer topicType) {
		this.topicType = topicType;
	}
	public String getQuestionText() {
		return questionText;
	}
	public Long getCategoryId() {
		return categoryId;
	}
	public void setCategoryId(Long categoryId) {
		this.categoryId = categoryId;
	}
	public void setQuestionText(String questionText) {
		this.questionText = questionText;
	}
	public String getChoosentopic() {
		return choosentopic;
	}
	public void setChoosentopic(String choosentopic) {
		this.choosentopic = choosentopic;
	}
	
}