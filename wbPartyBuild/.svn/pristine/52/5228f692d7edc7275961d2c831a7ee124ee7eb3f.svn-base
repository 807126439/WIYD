package com.wb.web.study.dto.examPaper;

public class ExamPaperDTO {
	
	private Long id;
	private String paperName;    //试卷名称
	private Integer paperType;
	private Integer examMinute; //考试时间（以分钟为单位）
	private Integer singleNum; //单选题数量
	private Integer multiNum;  //多选题数量
	private Integer blankNum;  //填空题数量
	private Integer judgeNum;  //判断题数量
	
	private String choosentopic;
	private String topicScore;
	private String topicText;
	private String topicTypeName;
	private String topicCName;
	
	
	
	private Long categoryId;


	public String getPaperName() {
		return paperName;
	}
	public void setPaperName(String paperName) {
		this.paperName = paperName;
	}
	public Integer getPaperType() {
		return paperType;
	}
	public void setPaperType(Integer paperType) {
		this.paperType = paperType;
	}
	public Integer getExamMinute() {
		return examMinute;
	}
	public void setExamMinute(Integer examMinute) {
		this.examMinute = examMinute;
	}
	public Integer getSingleNum() {
		return singleNum;
	}
	public void setSingleNum(Integer singleNum) {
		this.singleNum = singleNum;
	}
	public Integer getMultiNum() {
		return multiNum;
	}
	public void setMultiNum(Integer multiNum) {
		this.multiNum = multiNum;
	}
	public Integer getBlankNum() {
		return blankNum;
	}
	public void setBlankNum(Integer blankNum) {
		this.blankNum = blankNum;
	}
	public Integer getJudgeNum() {
		return judgeNum;
	}
	public void setJudgeNum(Integer judgeNum) {
		this.judgeNum = judgeNum;
	}
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getChoosentopic() {
		return choosentopic;
	}
	public void setChoosentopic(String choosentopic) {
		this.choosentopic = choosentopic;
	}

	public Long getCategoryId() {
		return categoryId;
	}
	public void setCategoryId(Long categoryId) {
		this.categoryId = categoryId;
	}
	public String getTopicScore() {
		return topicScore;
	}
	public void setTopicScore(String topicScore) {
		this.topicScore = topicScore;
	}
	public String getTopicText() {
		return topicText;
	}
	public void setTopicText(String topicText) {
		this.topicText = topicText;
	}
	public String getTopicTypeName() {
		return topicTypeName;
	}
	public void setTopicTypeName(String topicTypeName) {
		this.topicTypeName = topicTypeName;
	}
	public String getTopicCName() {
		return topicCName;
	}
	public void setTopicCName(String topicCName) {
		this.topicCName = topicCName;
	}

	

	
	
}
