body{background-color: #eeeff2;}
.onlinetest-topbar{position: fixed;top:0;left:0;right:0;text-align: left;z-index: 100;background-color: #b40000;color: #FFF;line-height: 38px;font-size: 16px;}
.onlinetest-topbar .test-back{display:inline-block;float: left;height: 38px;width:38px;-webkit-tap-highlight-color:rgba(255,255,255,.2);}
.onlinetest-topbar .test-back:before{content: '';position: absolute;top: 50%;left: 14px;border: 2px solid #FFF;border-width: 2px 0 0 2px;width: 8px;height: 8px;margin: -5px 0 0 0;-webkit-transform: scale(1) rotate(-50deg) skew(-10deg) translateZ(0);transform: scale(1) rotate(-50deg) skew(-10deg) translateZ(0);}
.onlinetest-topbar .test-back{display:inline-block;float: left;height: 38px;width:38px;-webkit-tap-highlight-color:rgba(255,255,255,.2);}
.onlinetest-topbar .test-back:before{content: '';position: absolute;top: 50%;left: 14px;border: 2px solid #FFF;border-width: 2px 0 0 2px;width: 8px;height: 8px;margin: -5px 0 0 0;-webkit-transform: scale(1) rotate(-50deg) skew(-10deg) translateZ(0);transform: scale(1) rotate(-50deg) skew(-10deg) translateZ(0);}

.onlinetest-guide{position: fixed;bottom: 0;left: 0;right: 0;height: 80px;z-index: 999;}
.onlinetest-prev,.onlinetest-next,.onlinetest-opennav{float: left;width: 33.33%;display: inline-block;text-align: center;color: #FFF;height: 80px;padding-top:45px;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;-webkit-tap-highlight-color:rgba(255,255,255,.1)}
.onlinetest-prev{border-left: 1px solid #eeeff2;border-right: 1px solid #eeeff2;background:#B40000 url(../images/icon_prev.png) no-repeat center 15px;background-size: 30px 30px;}
.onlinetest-opennav{border-left: 1px solid #eeeff2;border-right: 1px solid #eeeff2;background:#990000 url(../images/icon_call.png) no-repeat center 15px;background-size: 30px 30px;}
.onlinetest-next{border-left: 1px solid #eeeff2;border-right: 1px solid #eeeff2;background:#B40000 url(../images/icon_next.png) no-repeat center 15px;background-size: 30px 30px;}

.onlinetest-navlist{display: none;position: fixed;z-index: 99999;top:50%;left: 50%;-webkit-transform:translate(-50%,-50%);-ms-transform:translate(-50%,-50%);-o-transform:translate(-50%,-50%);transform:translate(-50%,-50%);width: 280px;max-height: 96%;overflow: auto;background-color:#FFF;border:1px solid #b89f3b;}
.onlinetest-navlist.active{display: block;}
.onlinetest-navlist li{float: left;margin:10px;}
.onlinetest-navlist li a{display: inline-block;width: 34px;height:34px;border:1px solid #9a9a9a;color:#555;text-align: center;line-height: 34px;background-color:#fff;border-radius: 2px;-webkit-transition:all 0.6s;-o-transition:all 0.6s;transition:all 0.6s;}
.onlinetest-navlist li a:hover{border-color:#2196F3;color:#2196F3;}
.onlinetest-navlist li.wrong a{background-color:#D21D1C;color:#fff;border:1px solid #D21D1C	;}
.onlinetest-navlist li.right a{background-color:#27A72C;color:#fff;border:1px solid #27A72C;}


#onlinetest{position: absolute;top:38px;left: 0;right: 0;bottom: 80px;overflow: auto;padding:0 10px;}
.onlinetest-controll{padding: 4px 0;line-height:30px;font-size:16px;color:#000;text-align: center;}
.questionnum {float: left; font-family: "Arial","Tahoma","Times New Roman";}
.onlinetest-controll strong{float: right;font-weight: normal;font-family: "Arial","Tahoma","Times New Roman";}
.onlinetest-controll .btngroup{position: absolute;top: 5px;left: 50%;-webkit-transform:translateX(-50%);-ms-transform:translateX(-50%);-o-transform:translateX(-50%);transform:translateX(-50%);}
.onlinetest-controll .btngroup a{color:#b40100;display: inline-block;float: left;background-color: #FFF;border: 1px solid #b40100;line-height:24px;vertical-align: top;padding:0 12px;-webkit-tap-highlight-color:rgba(33, 150, 243, .1);font-size:14px;}
.onlinetest-controll .btngroup .btn-pause {border-right:0;border-radius: 6px 0 0 6px;}
.btn-their{border-radius:0 6px 6px 0;}
.btn-pause span+span{display: none;}
.btn-pause.pausing span+span{display: block;}
.btn-pause.pausing span:first-child{display: none;}
.onlinetest-controll .btngroup .btn-pause.disabled,.onlinetest-controll .btngroup .btn-their.disabled{color:#999;border-color:#AAA;}


.onlinetest-ct{font-size:16px;color:#000;}
.question{display: none;}
.question.current{display: block;}
.question-title{color: #000;line-height:26px;padding:10px;background-color: #FFF;}
.question-options,.anwsergroup{list-style-type:upper-latin;padding:12px 12px 12px 60px;margin:12px 0;background-color:#FFF; }
.trueorfalse .question-options{list-style-type:none;}
.anwsergroup{list-style-type:lower-roman;}
.question-options li{position: relative;line-height:32px;padding-left: 6px;cursor:pointer;}
.anwsergroup li {position: relative;padding-left: 6px;}
.question-options li:hover{background-color:#f2f2f2;}
.question-options li:before,.anwsergroup li:before{content:"";display: inline-block;width:18px;height:32px;background:url('../images/right-wrong2.png') no-repeat 0px 8px/105px;position: absolute;left:-48px;}
.question-options .selected:before{background-position:-64px 8px;}
.question-options .right:before{background-position:-20px 8px;}
.morechoice .selected.right:before{background-position:-86px 8px;}
.question-options .wrong:before{background-position:-20px 8px;}
.question-options .selected.wrong:before{background-position:-42px 8px;}
.anwsergroup .right:before{background-position:-20px 8px;}
.anwsergroup .wrong:before{background-position:-42px 8px;}
.anwsergroup li{margin-bottom:12px;}
.anwsergroup input{width: 100%; height: 32px; -webkit-box-sizing: border-box;-moz-box-sizing: border-box;box-sizing: border-box;border:1px solid transparent;border-bottom:1px solid #777;line-height:30px;font-size:16px;}
.question .btn-submit{line-height:28px;display: block;background-color: #B40000;border: 1px solid #DDD;color: #FFF;border-radius:4px;text-align: center;}
.disabled .btn-submit,.btn-submit.disabled{background: #E9E9E9;color:#999;border-color:#AAA;display: none;}
.singlechoice .btn-submit{display: none;}
.trueorfalse .btn-submit{display: none;}
.question-result{color:#b40100;line-height:38px;padding:0 14px;background-color:#fdedcc;border:1px dashed #edd470;margin: 18px 0;}
.question-result a{text-decoration: underline;color:#666;font-size:14px;margin-left:10px;}
.onlinetest-ft{background-color:#f6f6f6;font-size:16px;line-height:30px;}

.onlinetest-statistics{text-align: center;font-size: 14px;}
.onlinetest-statistics em{color: #13a100;font-style: normal;font-weight: bold;}
.onlinetest-statistics strong{color: #dc0100;font-weight: bold;}
.analysislist{display: none;}


.mylayer-main{vertical-align: middle;position: relative;border: 1px solid #d8a400;}
.mylayer-hd{content: "";clear: both;background: #fff;font-size: 20px;line-height:34px;}
.mylayer-main>.mylayer-close{color:#fff;background-color: #b06a2e;display: inline-block;width:22px;height:22px;border-radius: 4px;font-size:22px;line-height:20px;cursor:pointer;position: absolute;top:8px;right:8px;text-align: center;}
.mylayer-main>.mylayer-close:before{content: '';display: block;height: 12px;width: 2px;position: absolute;top:5px;left: 50%;background: #FFF;transform: rotate(45deg);margin: 0 0 0 -1px;}
.mylayer-main>.mylayer-close:after{content: '';display: block;height: 12px;width: 2px;position: absolute;top:5px;left: 50%;background: #FFF;transform: rotate(-45deg);margin:  0 0 0 -1px;}
.mylayer-main .btns{padding:0 !important;border-top:1px solid #B06A2E;height:40px;margin-top: 8px;}
.mylayer-main .btns a{float: left;width:50%;line-height:40px;text-align:center;color:#B06A2E;font-size:16px;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;background-color:#FBF6F2;}
.mylayer-main .btns a:hover{background-color:#B06A2E;color:#FFF; }
.mylayer-main .btns a+a{border-left:1px solid #B06A2E;}

#mylayer1{text-align: left;padding:15px 15px 20px 15px;background: #fff;}
#mylayer1 .tt,#mylayer1 .subtt{color:#b16a2a;}
#mylayer1 .tt{font-size:24px;font-weight:bold;letter-spacing:2px;line-height:1;}
#mylayer1 .subtt{font-size:8px;line-height:24px;}
#mylayer1 .ct{min-height:100px;max-height:400px;border:1px solid #d8a400;overflow: auto;margin-top:10px;padding:8px 12px;font-size:14px;}

#mylayer1 .ct p{line-height:24px;text-indent: 2em;}
#mylayer1 .ct .opt{font-weight: bold;text-indent: 0;margin-top:4px;}

.mylayer-msg{padding-top:6px;font-size:16px;line-height:30px;color:#222;overflow: hidden;overflow: visible;}
.mylayer-msg>div:first-child{font-size:16px;padding:0 30px 0 16px;color:#9C5E28;}
.mylayer-msg>div:first-child+div,.mylayer-msg>div:first-child+div+div{text-indent:2em;padding:4px 16px;font-size:16px;line-height:24px;}
.mylayer-msg .wrong{color:#dc0100;font-size:20px;margin:0 4px;}
.mylayer-msg .right{color:#13a100;font-size:20px;margin:0 4px;}
.mylayer-msg .totalscore,.mylayer-msg .totaltime{color:#185F86;}
.mylayer-msg .usetime{color: #029289;}