package com.wb.web.workflow.dto.task;

import java.util.Date;
import java.util.List;

import com.wb.web.workflow.dto.attachment.AttachmentDTO;

public class TaskInfo {

	private Long id;
	private String name;							//任务名称
	private String taskExplain;						//任务说明
	private Short type;								//任务类型
	private String owner;							//发起人
	private Date dealTime;							//执行时间
	private Short status;							//状态 0：未完成  1:已完成
		
	private List<AttachmentDTO> attachmens;

	
	
	
	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getTaskExplain() {
		return taskExplain;
	}

	public void setTaskExplain(String taskExplain) {
		this.taskExplain = taskExplain;
	}

	public Short getType() {
		return type;
	}

	public void setType(Short type) {
		this.type = type;
	}

	public String getOwner() {
		return owner;
	}

	public void setOwner(String owner) {
		this.owner = owner;
	}

	public Date getDealTime() {
		return dealTime;
	}

	public void setDealTime(Date dealTime) {
		this.dealTime = dealTime;
	}

	public Short getStatus() {
		return status;
	}

	public void setStatus(Short status) {
		this.status = status;
	}

	public List<AttachmentDTO> getAttachmens() {
		return attachmens;
	}

	public void setAttachmens(List<AttachmentDTO> attachmens) {
		this.attachmens = attachmens;
	}
	
	
	
}
