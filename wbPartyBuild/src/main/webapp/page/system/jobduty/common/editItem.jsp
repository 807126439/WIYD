<%@ page language="java" pageEncoding="utf-8"%>

 <div class="row cl">
	      <label class="form-label col-xs-3 col-sm-3"><span class="c-red">*</span>职务编号：</label>
	      <div class="formControls col-xs-6 col-sm-6">
	        <input type="text" class="input-text" value="${jobDutyItem.jobNum}"  name="jobNum" datatype="*2-30" nullmsg="职务编号不能为空">
	      </div>
	      <div class="col-xs-3"> </div>
	    </div>
	    
	     <div class="row cl">
	      <label class="form-label col-xs-3 col-sm-3"><span class="c-red">*</span>职务名称：</label>
	      <div class="formControls col-xs-6 col-sm-6">
	        <input type="text" class="input-text" value="${jobDutyItem.jobName}"  name="jobName" datatype="*2-30" nullmsg="职务名称不能为空">
	      </div>
	      <div class="col-xs-3 col-sm-3"> </div>
	    </div>
	    
	    
	       
	     <div class="row cl">
	      <label class="form-label col-xs-3 col-sm-3"><span class="c-red">*</span>职务编码：</label>
	      <div class="formControls col-xs-6 col-sm-6">
	        <input type="text" class="input-text" value="${jobDutyItem.jobCode}"  name="jobCode" datatype="*2-30" nullmsg="职务编码不能为空">
	      </div>
	      <div class="col-xs-3 col-sm-3"> </div>
	    </div>
    
	    
	    <div class="row cl">
	      <label class="form-label col-xs-3 col-sm-3">级别：</label>
	      <div class="formControls col-xs-6 col-sm-6">
	        <input type="text" class="input-text"  name="level" value="${jobDutyItem.level}"   datatype="n" ignore="ignore"    />
	      </div>
		 <div class="col-xs-3 col-sm-3"> </div>
	    </div>
 

	 
	    
		<div class="row cl">
			<label class="form-label col-xs-3 col-sm-3">备注：</label>
			<div class="formControls col-xs-6 col-sm-6">
				<textarea name="jobMemo" cols="" rows="" class="textarea" datatype="*2-200"  ignore="ignore"  onKeyUp="textarealength(this,200)">${jobDutyItem.jobMemo}</textarea>
				<p class="textarea-numberbar"><em class="textarea-length">0</em>/200</p>
			</div>
			<div class="col-xs-3 col-sm-3"> </div>
		</div>
